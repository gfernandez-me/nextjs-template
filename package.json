{
  "name": "nextjs_template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env -- sh -c 'next dev --turbopack'",
    "lint": "next lint",
    "clean": "rm -rf .next && echo 'Cleared Next.js caches'",
    "build": "next build",
    "typecheck": "tsc --noEmit",
    "setup:env": "node -e \"const fs=require('fs'); if(!fs.existsSync('.env')&&fs.existsSync('.env.example')) fs.copyFileSync('.env.example','.env');\"",
    "studio": "dotenv -e .env -- prisma studio",
    "db:generate": "dotenv -e .env -- prisma generate",
    "db:migrate:new": "dotenv -e .env -- prisma migrate dev --create-only=false",
    "db:migrate:dev": "dotenv -e .env -- prisma migrate dev",
    "db:migrate:status": "dotenv -e .env -- prisma migrate status",
    "db:reset": "dotenv -e .env -- prisma migrate reset --force",
    "db:seed": "dotenv -e .env -- npx tsx prisma/seed.ts",
    "db:validate": "dotenv -e .env -- prisma validate"
  },
  "dependencies": {
    "@conform-to/react": "^1.8.2",
    "@conform-to/zod": "^1.8.2",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.2.1",
    "@prisma/client": "^6.14.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toggle": "^1.1.2",
    "@radix-ui/react-toggle-group": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@tabler/icons-react": "^3.31.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-table": "^8.21.2",
    "autoprefixer": "^10.4.21",
    "better-auth": "^1.2.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.483.0",
    "next": "15.3.4",
    "next-themes": "^0.4.6",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "recharts": "^2.15.1",
    "sonner": "^2.0.1",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.2.4",
    "vaul": "^1.1.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^22.17.2",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "dotenv-cli": "^10.0.0",
    "esbuild": "^0.23.1",
    "eslint": "^9.33.0",
    "eslint-config-next": "15.4.5",
    "prisma": "^6.14.0",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  },
  "overrides": {
    "@types/node": "^22.17.2"
  }
}
